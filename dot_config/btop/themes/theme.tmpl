# Btop Theme - Generated from active palette
# Theme: violet-lake

# Main background, empty for terminal default (transparency support)
theme[main_bg]=""

# Main text color
theme[main_fg]="#f6f3f1"

# Title color for boxes
theme[title]="#ffffff"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#3fbfbf"

# Background color of selected item in processes box
theme[selected_bg]="#49254c"

# Foreground color of selected item in processes box
theme[selected_fg]="#ffffff"

# Color of inactive/disabled text
theme[inactive_fg]="#e6ddd8"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#51a8d4"

# Cpu box outline color
theme[cpu_box]="#603460"

# Memory/disks box outline color
theme[mem_box]="#603460"

# Net up/down box outline color
theme[net_box]="#603460"

# Processes box outline color
theme[proc_box]="#603460"

# Box divider line and small boxes line color
theme[div_line]="#673569"

# Temperature graph colors (cool to hot)
theme[temp_start]="#3fbfbf"
theme[temp_mid]="#dca13b"
theme[temp_end]="#e28181"

# CPU graph colors (low to high usage)
theme[cpu_start]="#3ebb3e"
theme[cpu_mid]="#dca13b"
theme[cpu_end]="#e28181"

# Mem/Disk free meter
theme[free_start]="#3ebb3e"
theme[free_mid]="#4bb04b"
theme[free_end]="#74d174"

# Mem/Disk cached meter
theme[cached_start]="#fbbba3"
theme[cached_mid]="#ef8862"
theme[cached_end]="#fef1ec"

# Mem/Disk available meter
theme[available_start]="#938ad7"
theme[available_mid]="#9b94cf"
theme[available_end]="#c7c3ea"

# Mem/Disk used meter
theme[used_start]="#e28181"
theme[used_mid]="#d47b7b"
theme[used_end]="#edb2b2"

# Download graph colors
theme[download_start]="#3fbfbf"
theme[download_mid]="#938ad7"
theme[download_end]="#fbbba3"

# Upload graph colors
theme[upload_start]="#3ebb3e"
theme[upload_mid]="#dca13b"
theme[upload_end]="#e28181"
